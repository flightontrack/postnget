<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <PublishProvider>AzureWebSite</PublishProvider>
    <LastUsedBuildConfiguration>Debug</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://postnget.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>postnget.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>postnget</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>False</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$postnget</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="MSSQLConnectionString" Order="1" Enabled="False">
          <Destination Path="Data Source=tcp:postnget-server.database.windows.net,1433;Initial Catalog=flighttrack-db;Integrated Security=False;Persist Security Info=True;User ID=gapilot@postnget-server;Password=naishpark.size.11;Connect Timeout=30;Encrypt=True" Name="" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=.\MSSQLSERVER_2014;Initial Catalog=flighttrack-db-test;Integrated Security=False;Persist Security Info=True;User ID=gapilot;Password=naishpark.size.11" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\MSSQLConnectionString_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="Data Source=.\MSSQLSERVER_2014;Initial Catalog=flighttrack-db-test;Integrated Security=False;Persist Security Info=True;User ID=gapilot;Password=naishpark.size.11" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
    <PrecompileBeforePublish>True</PrecompileBeforePublish>
    <EnableUpdateable>True</EnableUpdateable>
    <DebugSymbols>False</DebugSymbols>
    <WDPMergeOption>DonotMerge</WDPMergeOption>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)MSSQLConnectionString-Web.config Connection String">
      <ParameterValue>Data Source=tcp:postnget-server.database.windows.net,1433;Initial Catalog=flighttrack-db;Integrated Security=False;Persist Security Info=True;User ID=gapilot@postnget-server;Password=naishpark.size.11;Connect Timeout=30;Encrypt=True</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>